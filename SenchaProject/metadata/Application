                    "function xmlToJson(xml) {",
                    "//console.log('xml');",
                    "\tvar obj = {};",
                    "",
                    "\tif (xml.nodeType == 1) {",
                    "",
                    "\t\tif (xml.attributes.length > 0) {",
                    "\t\tobj[\"attributes\"] = {};",
                    "\t\t\tfor (var j = 0; j < xml.attributes.length; j++) {",
                    "\t\t\t\tvar attribute = xml.attributes.item(j);",
                    "\t\t\t\tobj[\"attributes\"][attribute.nodeName] = attribute.value;//nodeValue",
                    "\t\t\t}",
                    "\t\t}",
                    "\t} else if (xml.nodeType == 3) { // text",
                    "\t\tobj = xml.nodeValue;",
                    "\t}",
                    "",
                    "\t// do children",
                    "\tif (xml.hasChildNodes()) {",
                    "\t\tfor(var i = 0; i < xml.childNodes.length; i++) {",
                    "\t\t\tvar item = xml.childNodes.item(i);",
                    "\t\t\tvar nodeName = item.nodeName;",
                    "\t\t\tif (typeof(obj[nodeName]) == \"undefined\") {",
                    "\t\t\t\tobj[nodeName] = xmlToJson(item);",
                    "\t\t\t} else {",
                    "\t\t\t\tif (typeof(obj[nodeName].push) == \"undefined\") {",
                    "\t\t\t\t\tvar old = obj[nodeName];",
                    "\t\t\t\t\tobj[nodeName] = [];",
                    "\t\t\t\t\tobj[nodeName].push(old);",
                    "\t\t\t\t}",
                    "\t\t\t\tobj[nodeName].push(xmlToJson(item));",
                    "\t\t\t}",
                    "\t\t}",
                    "\t}",
                    "   // console.log('returning');",
                    "   // console.log(obj);",
                    "\treturn obj;",
