                                    "",
                                    "function createPostList(postStore){",
                                    "    ",
                                    "    ",
                                    "    Ext.getCmp(site).add(",
                                    "    {",
                                    "                        xtype: 'dataview',",
                                    "                        itemHeight: '80',",
                                    "                        height: '100%',",
                                    "                       // id: 'siteList',",
                                    "                       // itemId: 'mydataview5',",
                                    "                        itemTpl: [",
                                    "                            '<div class=\"menuList\" style=\"color : gray; border-left: 2px solid {leftColor};line-height : 3em;\">',",
                                    "                            '    <table width=\"100%\">',",
                                    "                            '        <tr>',",
                                    "                            '            <td>&nbsp;',",
                                    "                            '                <tpl for=\"name\"> </tpl></td>',",
                                    "                            '            <td class = \"fontFamilyCath\" style=\"width : 80%; font-family  : CenturyGothic, AppleGothic, sans-serif;\">{postName}</td>',",
                                    "                            '            <td><img align= \"right\" style=\" width : 14px; height : 19px;\" src=\"img/img/arrow_right.png\"></td>',",
                                    "                            '            <td>&nbsp;</td>',",
                                    "                            '        </tr>',",
                                    "                            '    </table>   ',",
                                    "                            '</div>'",
                                    "                        ],",
                                    "                        store: postStore,",
                                    "                        listeners: {",
                                    "                            itemtap: function(dataview, index, target, record,e, eOpts) {",
                                    "                                ",
                                    "                                console.log(record);",
                                    "                                ",
                                    "                                ",
                                    "                                ",
                                    "                                ",
                                    "                            },//End itemTap",
                                    "                            afterrender : function(cmp){",
                                    "                               // alert();",
                                    "                               cmp.refresh();",
                                    "                            }",
                                    "                          }//end listners",
                                    "                    }",
                                    "    ",
                                    "    );",
                                    "    ",
                                    "    ",
                                    "    ",
                                    "    ",
                                    "    ",
                                    "}"
                                ],
                                "name": "itemtap"
                            },
                            "designerId": "96e61404-879a-4265-9a89-046599db52e0"
                        }
                    ]
                }
            ]
        },
